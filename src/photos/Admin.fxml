<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.AdminController">
    <children>

        <!-- ListView of current users -->
        <Label layoutX="84.0" layoutY="14.0" prefHeight="29.0" prefWidth="147.0" text="Current Users"
               style="-fx-font-family:Roboto; -fx-font-size:24.0"/>
        <ListView layoutX="28.0" layoutY="44.0" prefHeight="291.0" prefWidth="258.0"/>

        <!-- Delete button -->
        <Button fx:id="delete_button" layoutX="28.0" layoutY="336.0" mnemonicParsing="false" prefHeight="39.0"
                prefWidth="258.0"
                text="delete" style="-fx-font-family:Roboto; -fx-font-size:14.0"/>

        <!-- Create new users -->
        <Label layoutX="383.0" layoutY="14.0" prefHeight="29.0" prefWidth="127.0" text="Create New"
               style="-fx-font-family:Roboto; -fx-font-size:24.0"/>
        <Label layoutX="314.0" layoutY="65.0" prefHeight="29.0" prefWidth="186.0" text="New Username:"
               style="-fx-font-family:Roboto; -fx-font-size:18.0"/>
        <TextField fx:id="new_username" layoutX="314.0" layoutY="94.0" prefHeight="29.0" prefWidth="258.0"/>
        <Label layoutX="314.0" layoutY="132.0" prefHeight="29.0" prefWidth="180.0" text="New Password:"
               style="-fx-font-family:Roboto; -fx-font-size:18.0"/>
        <PasswordField fx:id="new_password" layoutX="314.0" layoutY="160.0" prefHeight="29.0" prefWidth="258.0"/>
        <Label layoutX="314.0" layoutY="199.0" prefHeight="29.0" prefWidth="180.0" text="Verify Password:"
               style="-fx-font-family:Roboto; -fx-font-size:18.0"/>
        <PasswordField fx:id="verify_password" layoutX="314.0" layoutY="228.0" prefHeight="29.0" prefWidth="258.0"/>
        <Button layoutX="314.0" layoutY="270.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="258.0"
                text="create" style="-fx-font-family:Roboto; -fx-font-size:14.0"/>

        <!-- Logout -->
        <Button fx:id="logout" layoutX="314.0" layoutY="336.0" mnemonicParsing="false" prefHeight="39.0"
                prefWidth="258.0"
                text="logout" style="-fx-font-family:Roboto; -fx-font-size:14.0"/>

        <!-- Misc. -->
        <Line endX="100.0" layoutX="443.0" layoutY="323.0" startX="-100.0"/>
    </children>
</AnchorPane>
